{
  "expo": {
    "name": "Diagno_Next_Informatics",
    "slug": "Diagno_Next_Informatics",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ui/mainIcon.jpeg",
    "scheme": "diagnonextinformatics",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/ui/icon.png" 
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ui/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#rgb(230, 244, 254)",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    }
  }
}
